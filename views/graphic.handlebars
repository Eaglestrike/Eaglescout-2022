<nav class="blue darken-2">
    <div class="nav-wrapper container">
        <div class="col s12">
            <a href="/scout" class="breadcrumb">Scout</a>
            <a href="/scout/graphic" class="breadcrumb">Graphics</a>
        </div>
    </div>
</nav>

<div class="section no-pad-bot" id="index-banner">
    <div class="container">
        <h1 class="header center blue-text text-darken-4">Eaglescout<span class="bullet"> &bull; </span><span class="subtitle">Graphics</span></h1>

        <div class="input-field col s12">
            <select id="events-select" multiple>
                {{#if events}}
                    {{#each events}}
                        {{#if current }}
                            <option value="{{ key }}" selected>{{ name }}</option>
                        {{else}}
                            <option value="{{ key }}">{{ name }}</option>
                        {{/if}}
                    {{/each}}
                {{/if}}
            </select>
        <label for="events-select">Select Events</label>
        </div>
        <div class="center">
            <button id="filters-submit" class="btn waves-effect waves-light red">Apply Filters</button>
        </div>
        <canvas id="chart1" width="400" height="400">
        </canvas>
    </div>
</div>